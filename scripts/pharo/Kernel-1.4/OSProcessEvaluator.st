UIManager default progressBarEnabled: true.Gofer new  squeaksource: 'OSProcess';  package: 'OSProcess';  load.(Smalltalk at: #OSProcess) helloWorld. !Smalltalk snapshot: true andQuit: true."OSProcess thisOSProcess stdOut nextPutAll: 'Hello world'; lf."!| input ioHandle inputString line result |input := OSProcess thisOSProcess stdIn.input ifNil: [OSProcess noAccessorAvailable. ^ nil].ioHandle := input ioHandle.OSProcess accessor setBlocking: ioHandle.[  [ line := ''.  [ inputString := OSProcess thisOSProcess stdIn next: 1.  line := line, inputString.  inputString = String lf ] whileFalse.  result := Compiler evaluate: line.    OSProcess thisOSProcess stdOut nextPutAll: result asString; lf.  ] ifError: [:error | OSProcess thisOSProcess stdOut nextPutAll: error asString; lf.].true] whileTrue.